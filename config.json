{
  "xml_feeds": {
    "gastromarket": {
      "url": "https://www.gastromarket.sk/web/content/259710/B2B_product_feed_GastroMarket.xml",
      "root_element": "PRODUKT",
      "mapping": {
        "KATALOG_CISLO": "Kat. číslo",
        "MENO": "Názov tovaru",
        "POPIS": "Krátky popis",
        "CENA": "Bežná cena",
        "VYROBCA": "Výrobca",
        "KATEGORIA_KOMPLET": "Hlavna kategória",
        "OBRAZOK": "Obrázky"
      }
    },
    "forgastro": {
      "url": "https://www.forgastro.sk/xml",
      "root_element": "product",
      "mapping": {
        "product_sku": "Kat. číslo",
        "product_name": "Názov tovaru",
        "product_price": "Bežná cena",
        "manufacturer": "Výrobca",
        "category": "Hlavna kategória",
        "product_s_desc": "Krátky popis",
        "product_desc": "Dlhý popis",
        "images/item/url": "Obrázky",
        "product_weight": "Váha"
      }
    }
  },
  "ai_enhancement": {
    "enabled": true,
    "model": "gemini-2.5-flash-lite",
    "batch_size": 30,
    "temperature": 0.1,
    "retry_attempts": 3,
    "retry_delay": 60,
    "max_parallel_calls": 5
  },
  "final_csv_columns": [
    "Kat. číslo rodiča",
    "Kat. číslo",
    "Viditeľný",
    "Výrobca",
    "Názov tovaru",
    "Hlavna kategória",
    "Krátky popis",
    "Dlhý popis",
    "Bežná cena",
    "Váha",
    "Obrázky",
    "Súvisiace tovary",
    "SEO titulka",
    "SEO popis",
    "SEO kľúčové slová",
    "Spracovane AI",
    "AI_Processed_Date"
  ]
}